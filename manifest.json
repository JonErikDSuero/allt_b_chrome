{
  "manifest_version": 2,

  "name": "Alternative Bookmark",
  "description": "This extension is your alternative bookmark.",
  "version": "1.0",

  "permissions": [
    "tabs",
    "contextMenus",
    "bookmarks"
  ],
  "background": {
    "scripts": ["right_click.js", "jquery-2.1.1.min.js"]
  },
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  }
}